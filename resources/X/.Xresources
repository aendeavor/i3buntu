### ~/.Xresorces

############################################ XFT

Xft.dpi:                    96
Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

############################################ Color settings

*background: #002b36
*foreground: #657b83
!!*fading: 40
*fadeColor: #002b36
*cursorColor: #93a1a1
*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1

!! black dark/light
*color0: #073642
*color8: #002b36

!! red dark/light
*color1: #dc322f
*color9: #cb4b16

!! green dark/light
*color2: #859900
*color10: #586e75

!! yellow dark/light
*color3: #b58900
*color11: #657b83

!! blue dark/light
*color4: #268bd2
*color12: #839496

!! magenta dark/light
*color5: #d33682
*color13: #6c71c4

!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1

!! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

############################################ URXVT

URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.loginShell:           true
URxvt.internalBorder:       7
URxvt.lineSpace:            0
URxvt.letterspace:          -1
URxvt.cursorBlink:          0

### transparency - true or false (default)
URxvt*transparent: true

### shading - 0 to 99 darkens, 101 to 200 lightens
URxvt*shading: 15

URxvt*saveLines: 12000

URxvt*foreground: #e5e8e7
URxvt*background: #1b202a

### OpenSans Linux font
URxvt*font: xft:OpenSans:pixelsize=14
URxvt*boldFont: xft:OpenSans:bold:pixelsize=14

### scrolling
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollstyle: plain
URxvt.scrollWithBuffer:     true

### Extensions & Copy
URxvt.perl-ext-common: default,clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.C-c: perl:clipboard:copy
URxvt.keysym.C-v: perl:clipboard:paste
URxvt.clipboard.copycmd: xsel -ib